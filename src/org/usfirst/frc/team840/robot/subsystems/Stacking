package org.usfirst.frc.team840.robot.subsystems;
import edu.wpi.first.wpilibj.command.Subsystem;
/**
*
*/

//qaz: Solenoid name, needs changing to whatever it's supposed to be
public void init {
    Solenoid qaz = new Solenoid(1,2,3);//three in total
    Compressor com = new Compressor(1,2);
    }
    public void stacking() {
        com.start();
        if (isAButtonPressed = true) {
            qaz.set(qaz.Value.kForward);//charge
            Timer.delay(10.0);
        }
        if (isBButtonPressed = true) {
            qaz.set(qaz.Value.kReverse);
            Timer.delay(10.0);
        }
        //com.stop();
    }
// Put methods for controlling this subsystem
// here. Call these from Commands.

